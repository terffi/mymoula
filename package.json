{
  "name": "mymoula",
  "version": "0.0.1",
  "private": true,
  "description": "yes",
  "homepage": "https://github.com/terffi/mymoula#readme",
  "bugs": {
    "url": "https://github.com/terffi/mymoula/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terffi/mymoula.git"
  },
  "license": "ISC",
  "author": "justsam^^",
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "turbo build",
    "vercel-build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "start:backend": "node apps/api/dist/server.js",
    "build-back": "cd ./app/back && yarn type-check && yarn pre-commit-build",
    "commit-build-back": "git add ./app/back/dist"
  },
  "pre-commit": [
    "build-back",
    "commit-build-back"
  ],
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "latest"
  },
  "workspaces": [
    "app/*",
    "packages/*"
  ],
  "dependencies": {
    "tsx": "^4.20.3"
  }
}