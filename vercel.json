{
    "version": 2,
    "buildCommand": "yarn build",
    "functions": {
        "dist/back/server.js": {
            "includeFiles": "dist/back/**"
        }
    },
    "outputDirectory": "dist/front",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/dist/back/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/dist/front/index.html"
        }
    ]
}