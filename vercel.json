{
    "version": 2,
    "builds": [
        {
            "src": "app/back/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "app/front/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "app/back/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "app/back/index.ts"
        }
    ]
}