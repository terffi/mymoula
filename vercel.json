{
    "version": 2,
    "buildCommand": "yarn build",
    "builds": [
        {
            "src": "app/back/dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "app/back/dist/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "app/back/dist/index.js"
        }
    ]
}